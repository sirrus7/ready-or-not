{
  "name": "vite-react-typescript-starter",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "lint": "eslint . --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint . --fix",
    "lint:supabase": "eslint . --format=compact | grep -E 'supabase|@supabase' || true",
    "lint:check-services": "echo '🔍 Checking for direct Supabase usage...' && npm run lint:supabase && echo '✅ Check complete! Any violations shown above.'",
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:sso": "vitest run src/services/__tests__/sso-service.test.ts",
    "test:auth": "vitest run src/components/auth/__tests__/",
    "test:utils": "vitest run src/components/auth/__tests__/utils.test.ts",
    "test:jwt": "vitest run src/services/__tests__/jwt-service.test.ts",
    "test:debug": "vitest run --reporter=verbose"
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.45.0",
    "@types/qrcode": "^1.5.5",
    "html2canvas": "^1.4.1",
    "jose": "^6.0.12",
    "jspdf": "^2.5.1",
    "lucide-react": "^0.344.0",
    "openai": "^5.3.0",
    "qrcode": "^1.5.4",
    "react": "^18.3.1",
    "react-archer": "^4.4.0",
    "react-dom": "^18.3.1",
    "react-markdown": "^10.1.0",
    "react-router-dom": "^6.22.2",
    "recharts": "^3.0.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.9.1",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.1.0",
    "@testing-library/user-event": "^14.5.2",
    "@types/jspdf": "^1.3.3",
    "@types/react": "^18.3.5",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react": "^4.6.0",
    "autoprefixer": "^10.4.18",
    "eslint": "^9.9.1",
    "eslint-formatter-compact": "^8.40.0",
    "eslint-plugin-react-hooks": "^5.1.0-rc.0",
    "eslint-plugin-react-refresh": "^0.4.11",
    "globals": "^15.9.0",
    "jsdom": "^26.0.0",
    "postcss": "^8.4.35",
    "tailwindcss": "^3.4.1",
    "typescript": "^5.5.3",
    "typescript-eslint": "^8.3.0",
    "vite": "6.3.4",
    "vitest": "^3.2.4"
  }
}
